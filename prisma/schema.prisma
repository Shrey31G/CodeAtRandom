generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Analysis {
  id            String   @id @default(uuid())
  targetRole    String
  currentSkills String[]
  matchedSkills String[]
  missingSkills String[]
  createdAt     DateTime @default(now())
}
